<script type="text/javascript">
var deleteAdmin = function(id) {
  var confirmation = confirm('Are you sure to delete?');
  if(confirmation) {
    $.ajax({
      type: "post",
      data: {_id: id},
      url: "/backoffice/delete-admin",
      success: function(result) {
        window.location.href = "/backoffice/admin-user";
      }
    });
  }
}
